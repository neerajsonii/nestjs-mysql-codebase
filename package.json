{
    "name": "first-project",
    "version": "0.0.1",
    "description": "",
    "author": "Neeraj Soni",
    "license": "UNLICENSED",
    "scripts": {
        "build": "tsc",
        "start:debug": "nodemon",
        "start": "ts-node ./src/main.ts",
        "start:local": "NODE_ENV=local npm run start",
        "start:dev": "NODE_ENV=dev npm run start",
        "start:prod": "NODE_ENV=prod npm run start",
        "migration:run": "node_modules/.bin/sequelize db:migrate",
        "migration:generate": "sequelize-cli migration:generate",
        "migration:undo:latest": "sequelize-cli db:migrate:undo",
        "migration:undo:all": "sequelize-cli db:migrate:undo:all",
        "seeds:run": "sequelize-cli db:seed:all",
        "seeds:generate": "sequelize-cli seed:generate",
        "seeds:undo:latest": "sequelize-cli db:seed:undo",
        "seeds:undo:all": "sequelize-cli db:seed:undo:all",
        "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json --project ./tsconfig.json -t codeFrame",
        "lint:fix": "./node_modules/tslint/bin/tslint -c ./tslint.json --fix --project ./tsconfig.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prettier": "./node_modules/prettier/bin-prettier.js --debug-check \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
        "prettier:fix": "./node_modules/prettier/bin-prettier.js --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\""
    },
    "dependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/config": "^0.6.3",
        "@nestjs/core": "^7.0.0",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/sequelize": "^0.2.0",
        "@nestjs/swagger": "^4.7.12",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "csurf": "^1.11.0",
        "express-rate-limit": "^5.1.3",
        "helmet": "^3.22.0",
        "mocha": "^8.3.0",
        "mysql2": "^2.2.5",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "sequelize": "^6.6.2",
        "sequelize-cli": "^6.2.0",
        "sequelize-typescript": "^2.1.0",
        "swagger-ui-express": "^4.1.4",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.11",
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.3",
        "@types/express-rate-limit": "^5.0.0",
        "@types/helmet": "0.0.46",
        "@types/jest": "25.1.4",
        "@types/node": "^13.9.1",
        "@types/sequelize": "^4.28.9",
        "@types/supertest": "^2.0.8",
        "husky": "^4.2.5",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "supertest": "^4.0.2",
        "ts-jest": "^26.5.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-etc": "^1.13.7",
        "typescript": "^3.7.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
